{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://spaceteam.at/schemas/date.schema.json",
  "title": "date",
  "description": "A date in the date-time format",
  "type": "string",
  "format": "date-time",
  "pattern": "\\d{4}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d+)?Z?",
  "minLength": 1
}
